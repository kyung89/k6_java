package practice.Practice_2024_02_23_1;

public class GugudanTest {

	public static void main(String[] args) {
		
		Gugudan ggd = new Gugudan();
		
		ggd.print(2); // 2단 출력
		System.out.println("-".repeat(15));

		ggd.print(3); // 3단 출력
		System.out.println("-".repeat(15));

		ggd.print(4); // 4단 출력
		System.out.println("-".repeat(15));
		
		System.out.println("End");
	}
}
